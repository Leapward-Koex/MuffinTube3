{
  "name": "MuffinTube",
  "version": "3.0.0",
  "private": true,
  "dependencies": {
    "assert": "^2.0.0",
    "body-parser": "~1.5.2",
    "bootstrap": "^5.1.3",
    "browser-id3-writer": "^4.4.0",
    "electron-json-storage": "^4.5.0",
    "electron-log": "^4.4.6",
    "esbuild": "^0.14.18",
    "ffmpeg-static": "^4.4.1",
    "lodash.throttle": "^4.1.1",
    "mv": "^2.1.1",
    "node-fetch": "^3.2.0",
    "querystring-es3": "^0.2.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-spring": "^9.4.4",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4",
    "youtube-dl-exec": "^2.0.2"
  },
  "scripts": {
    "start-react": "set BROWSER=none && react-app-rewired start",
    "start-electron": "concurrently --kill-others \"npm run build-electron && set ELECTRON_START_URL=http://localhost:3000 && electron --inspect=5858 .\"",
    "build-renderer": "react-app-rewired build",
    "build-electron": "node ./electron-esbuild.js",
    "build-electron-win": "npm run build-electron && mkdir build\\src && Xcopy /E /I /Y electron build\\electron",
    "package-win": "npm run build-renderer && npm run build-electron-win && electron-builder build -c.extraMetadata.main=build/electron/electron-bundle.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.3.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-json-storage": "^4.5.0",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mv": "^2.1.2",
    "@types/node": "^16.11.21",
    "@types/npm": "^7.19.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "react-app-rewired": "^2.1.11",
    "webpack-cli": "^4.9.2"
  },
  "main": "electron/electron-bundle.js",
  "homepage": "./",
  "build": {
    "appId": "com.tecktonikmuffin.muffintube",
    "productName": "MuffinTube 3",
    "files": [
      "build/**/*",
      "public/**/*"
    ]
  }
}
