  "scripts": {
    "start-react": "set BROWSER=none && react-app-rewired start",
    "start-electron": "concurrently --kill-others \"npm run build-electron && set ELECTRON_START_URL=http://localhost:3000 && electron --inspect-brk=5858 .\"",
    "build-renderer": "CI=false react-app-rewired build",
    "build-electron": "CI=false node ./electron-esbuild.js",
    "build-electron-win": "npm run build-electron && mkdir build\\src && Xcopy /E /I /Y electron build\\electron",
    "package-win": "CI=false npm run build-renderer && npm run build-electron-win && electron-builder build -c.extraMetadata.main=build/electron/electron-bundle.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/electron-json-storage": "^4.5.0",
    "@types/ffmpeg-static": "^3.0.1",
    "@types/lodash.orderby": "^4.6.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/mv": "^2.1.2",
    "@types/npm": "^7.19.0",
    "concurrently": "^7.0.0",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0",
    "react-app-rewired": "^2.1.11",
    "webpack-cli": "^4.9.2"
  },
  "main": "electron/electron-bundle.js",
  "homepage": "./",
  "build": {
    "appId": "com.tecktonikmuffin.muffintube",
    "productName": "MuffinTube 3",
    "files": [
      "build/**/*",
      "public/**/*"
    ]
  }
}
